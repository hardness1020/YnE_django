    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Run: Django Local DB",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
            ],
            "env": {
                "TRAMPOLINE_CI": "True",},
            "django": true,
        },
        {
            "name": "Test: Django local DB",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "test",
                // "django_user"
            ],
            "env": {
                "TRAMPOLINE_CI": "True",},
            "django": true
        },
        {
            "name": "Run: Django cloud dev DB",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
            ],
            "env": {
                "GOOGLE_CLOUD_PROJECT": "yne-django-dev",
                "USE_CLOUD_SQL_AUTH_PROXY": "true"},
            "django": true,
        },
        {
            "name": "Test: Django cloud dev DB",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "test",
                // "django_user"
            ],
            "env": {
                "GOOGLE_CLOUD_PROJECT": "yne-django-dev",
                "USE_CLOUD_SQL_AUTH_PROXY": "true"},
            "django": true
        },
    ]
}